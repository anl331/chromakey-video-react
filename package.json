{
  "name": "chromakey-video-react",
  "version": "0.1.0",
  "description": "Real-time chroma key video in React. No FFmpeg, no pre-processing, runs on the GPU.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "chroma-key",
    "green-screen",
    "video",
    "webgl",
    "gpu",
    "chromakey",
    "transparency",
    "shader"
  ],
  "author": "Alfredo Natal <anatal31@gmail.com> (https://x.com/_itsanl)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anl331/react-chromakey-video"
  },
  "homepage": "https://github.com/anl331/react-chromakey-video#readme",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
